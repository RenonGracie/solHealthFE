{
  "name": "sol-health-frontend",
  "private": true,
  "version": "1.1.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "format": "prettier --write .",
    "prepare": "husky",
    "generate-api": "openapi-typescript https://api.dev.solhealth.co/openapi/openapi.json --output src/api/types/api.ts",
    "release:patch": "yarn version --patch && git push && git push --tags",
    "release:minor": "yarn version --minor && git push && git push --tags",
    "release:major": "yarn version --major && git push && git push --tags"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx,json,css,scss,md}": "prettier --write"
  },
  "dependencies": {
    "@headlessui/react": "2.2.0",
    "@sentry/react": "9.8.0",
    "@tailwindcss/vite": "4.0.3",
    "@typeform/embed-react": "4.3.0",
    "axios": "1.7.9",
    "date-fns": "4.1.0",
    "date-fns-tz": "3.2.0",
    "framer-motion": "12.6.4",
    "lodash": "4.17.21",
    "react": "^18.3.1",
    "react-day-picker": "9.5.1",
    "react-dom": "^18.3.1",
    "react-gtm-module": "2.0.11",
    "tailwind-merge": "3.0.1",
    "tailwindcss": "4.0.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@sentry/vite-plugin": "3.2.2",
    "@testing-library/dom": "10.4.0",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "16.2.0",
    "@types/lodash": "4.17.15",
    "@types/node": "22.13.1",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@types/react-gtm-module": "2.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.17.0",
    "eslint-plugin-react": "7.37.4",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "globals": "^15.14.0",
    "husky": "9.1.7",
    "jsdom": "26.0.0",
    "lint-staged": "15.4.3",
    "openapi-typescript": "7.6.1",
    "patch-package": "8.0.0",
    "postinstall-postinstall": "2.1.0",
    "prettier": "3.4.2",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.18.2",
    "vite": "^6.0.5",
    "vite-plugin-svgr": "4.3.0",
    "vitest": "3.0.5"
  }
}
